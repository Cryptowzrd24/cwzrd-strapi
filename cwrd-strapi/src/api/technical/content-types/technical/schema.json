{
  "kind": "collectionType",
  "collectionName": "technicals",
  "info": {
    "singularName": "technical",
    "pluralName": "technicals",
    "displayName": "Technical"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "summary": {
      "type": "text"
    },
    "content": {
      "type": "richtext"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::auther.auther",
      "inversedBy": "technicals"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "technicals"
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "isFeatured": {
      "type": "boolean",
      "default": false
    },
    "isTrending": {
      "type": "boolean",
      "default": false
    },
    "readTimeMinutes": {
      "type": "integer"
    },
    "hash_tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::hash-tag.hash-tag",
      "inversedBy": "technicals"
    },
    "coins": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::coin.coin",
      "mappedBy": "technicals"
    },
    "support_levels": {
      "type": "json"
    },
    "resistance_levels": {
      "type": "json"
    },
    "market_sentiment": {
      "type": "enumeration",
      "enum": [
        "Bullish",
        "Bearish",
        "Neutral"
      ]
    },
    "focus_area": {
      "type": "enumeration",
      "enum": [
        "Lower Timeframe",
        "Weekly Transition"
      ]
    },
    "intraday_behavior": {
      "type": "blocks"
    },
    "tradingview_link": {
      "type": "string"
    },
    "doc_file": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
