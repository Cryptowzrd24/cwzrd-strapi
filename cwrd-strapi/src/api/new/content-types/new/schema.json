{
  "kind": "collectionType",
  "collectionName": "news",
  "info": {
    "singularName": "new",
    "pluralName": "news",
    "displayName": "New"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "coverImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "gallery": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "content": {
      "type": "richtext"
    },
    "views": {
      "type": "integer"
    },
    "isFeatured": {
      "type": "boolean"
    },
    "isTrending": {
      "type": "boolean",
      "default": false
    },
    "readTimeMinutes": {
      "type": "integer"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::auther.auther",
      "inversedBy": "news"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "news"
    },
    "hash_tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::hash-tag.hash-tag",
      "mappedBy": "news"
    },
    "summary": {
      "type": "text"
    },
    "coin": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::coin.coin",
      "inversedBy": "news"
    }
  }
}
