    },
    "priceChange1h": {
      "type": "decimal"
    },
    "priceChange24h": {
      "type": "decimal"
    },
    "priceChange7d": {
      "type": "decimal"
    },
    "marketCap": {
      "type": "decimal"
    },
    "marketCapChange": {
      "type": "decimal"
    },
    "chartImage": {
      "type": "media",
      "multiple": false,
