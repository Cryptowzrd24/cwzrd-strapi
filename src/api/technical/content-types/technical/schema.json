    "title": {
      "type": "string"
    },
    "excerpt": {
      "type": "string"
    },
    "symbol": {
      "type": "string"
    },
    "symbolPair": {
      "type": "string"
    },
    "assetType": {
      "type": "enumeration",
      "enum": [
        "crypto",
        "forex",
        "stock"
      ]
    },
    "analysisDate": {
      "type": "datetime"
    },
    "author": {
