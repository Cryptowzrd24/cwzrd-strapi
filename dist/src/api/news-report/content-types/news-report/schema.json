{
    "kind": "collectionType",
    "collectionName": "news_reports",
    "info": {
        "singularName": "news-report",
        "pluralName": "news-reports",
        "displayName": " News-report"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "titles": {
            "type": "string"
        },
        "slug": {
            "type": "uid"
        },
        "coverImage": {
            "type": "media",
            "multiple": false,
            "allowedTypes": [
                "images",
                "files",
                "videos",
                "audios"
            ]
        },
        "gallery": {
            "type": "media",
            "multiple": true,
            "allowedTypes": [
                "images",
                "files",
                "videos",
                "audios"
            ]
        },
        "views": {
            "type": "integer"
        },
        "isFeatured": {
            "type": "boolean",
            "default": false
        },
        "isTrending": {
            "type": "boolean",
            "default": false
        },
        "readTimeMinutes": {
            "type": "integer"
        },
        "body": {
            "type": "richtext"
        },
        "author": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::author.author",
            "inversedBy": "news_reports"
        },
        "categories": {
            "type": "relation",
            "relation": "manyToMany",
            "target": "api::category.category",
            "mappedBy": "news_reports"
        },
        "hash_tags": {
            "type": "relation",
            "relation": "manyToMany",
            "target": "api::tag.tag",
            "inversedBy": "news_reports"
        },
        "coin": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::coin.coin",
            "inversedBy": "news_reports"
        }
    }
}
